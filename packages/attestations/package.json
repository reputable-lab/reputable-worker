{
  "name": "attestation",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.js",
  "scripts": {
    "prisma:generate": "cd ../data-warehouse && prisma generate",
    "start:dev": "tsx --env-file ../../.env.dev ./src/reputationModel.ts",
    "start:prod": "tsx --env-file ../../.env.prod ./src/reputationModel.ts",
    "start-cron:dev": "tsx --env-file ../../.env.dev ./src/index.ts",
    "start-cron:prod": "tsx --env-file ../../.env.prod ./src/index.ts",
    "createEasSchema": "tsx --env-file ../../.env.prod ./src/utils/createNewSchema.ts",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"(src|tests)/**/*.ts\"",
    "check-format": "prettier --check \"(src|tests)/**/*.ts|tests/**/*.ts\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "npm run test -- --coverage",
    "clear:logs": "rimraf logs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ethereum-attestation-service/eas-sdk": "^2.7.0",
    "@ethsign/sp-sdk": "^0.7.7",
    "@logtail/node": "^0.4.21",
    "@logtail/winston": "^0.4.21",
    "@reputable/data-warehouse": "*",
    "better-sqlite3": "^11.1.2",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.4",
    "express": "^4.19.2",
    "rimraf": "^6.0.0",
    "tsx": "^4.6.2",
    "viem": "^2.21.45",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/jest": "^0.2.36",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "typescript": "^5.4.5"
  }
}
